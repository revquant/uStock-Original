{
  "manifest_version": 3,
  "name": "uStock Original",
  "description": "Simulates a fake stock price based on visited websites.",
  "version": "1.61803",
  "permissions": [
    "tabs",
    "scripting",
    "activeTab",
    "webNavigation",
    "storage"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon.png",
      "48": "icon.png",
      "128": "icon.png"
    }
  },
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  },
  "background": {
    "service_worker": "background.js"
  }
}
